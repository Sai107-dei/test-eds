    "id": "hero",
    "fields": [
      {
        "component": "container",
        "label": "primary-hero",
        "name": "hero banner",
        "valueType": "string",
        "fields": [
          {
            "component": "select",
            "valueType": "string",
            "name": "PrimarybackgroundType",
            "label": "Primary Background Type",
            "options": [
              {
                "name": "Image",
                "value": "imageSlide"
              },
              {
                "name": "Video",
                "value": "videoSlide"
              }
            ],
            "multi": false
          },
          {
            "component": "container",
            "label": "Image Details111",
            "name": "HeroImage",
            "valueType": "string",
            "condition": {
              "===": [
                {
                  "var": "PrimarybackgroundType"
                },
                "imageSlide"
              ]
            },
            "fields": [
              {
                "component": "reference",
                "name": "image",
                "label": "Image",
                "required" : true,
                "multi": false,
                "allowEmpty": false,
                "validation": {
                  "type": "mandatory"
                }
              },
              {
                "component": "text",
                "name": "imageAlt",
                "label": "Alt Text"
              }
            ]
          },
          {
            "component": "container",
            "label": "Video Details",
            "name": "HeroVideo",
            "valueType": "string",
            "condition": {
              "===": [
                {
                  "var": "PrimarybackgroundType"
                },
                "videoSlide"
              ]
            },
            "fields": [
              {
                "component": "text",
                "name": "videoId",
                "label": "Enter Video ID12345",
                "required": true
              }
            ]
          }
        ]
      },
      {
        "component": "text",
        "name": "text",
        "required": true,
        "label": "Title",
        "valueType": "string"
      },
