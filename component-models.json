    "id": "hero",
    "fields": [
      {
        "component": "select",
        "name": "PrimarybackgroundType",
        "label": "Primary Background Type",
        "description": "Selct Background Type Primary Hero",
        "required": true,
        "options": [
          {
            "name": "Image",
            "value": "imageSlide"
          },
          {
            "name": "Video",
            "value": "videoSlide"
          }
        ]
      },
      {
        "component": "text",
        "valueType": "string",
        "label": "Background Video",
        "name": "BackgroundVideo",
        "description": "Upload video for the Primary Hero",
        "required": true,
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "videoSlide"
          ]
        }
      },
      {
        "component": "boolean",
        "label": "Enable Overlay?",
        "name": "EnableOverlay",
        "valueType": "boolean",
        "description": "Enable Overlay video for the Primary Hero",
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "videoSlide"
          ]
        }
      },
      {
      "component": "text-input",
      "valueType": "string",
      "name": "text-input",
      "label": "Overlay Title",
      "required": true,
      "condition": {
          "===": [
            {
              "var": "EnableOverlay"
            },
            true
          ]
        }
      },
      {
        "component": "reference",
        "valueType": "string",
        "name": "BackgroundImage",
        "label": "Background Image",
        "description": "Upload Image for the Primary Hero",
        "required": true,
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "imageSlide"
          ]
        }
      },
      {
        "component": "text",
        "valueType": "string",
        "name": "Image Alt",
        "label": "Image Alt Text",
        "description": "Add Alt Text for Image Primary Hero",
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "imageSlide"
          ]
        }
      },
      {
        "component": "text",
        "name": "text",
        "label": "Title",
        "required": true,
        "valueType": "string",
        "description": "Enter main heading for the Primary Hero"
      },
      {
        "component": "richtext",
        "name": "description",
        "label": "Description",
        "valueType": "string",
        "description": "Enter description for additional context"
      },
      {
        "component": "select",
        "name": "targetPath",
        "label": "Target Path",
