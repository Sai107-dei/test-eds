        "name": "BackgroundVideo",
        "description": "Upload video for the Primary Hero",
        "required": true,
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "videoSlide"
          ]
        }
      },
      {
        "component": "boolean",
        "label": "Enable Hero Overlay",
        "name": "EnableOverlay",
        "valueType": "boolean",
        "description": "Select this checkbox to enable overlay",
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "videoSlide"
          ]
        }
      },
      {
      "component": "text-input",
      "valueType": "string",
      "name": "text-input",
      "label": "Overlay Title",
      "required": true,
      "condition": {
          "===": [
            {
              "var": "EnableOverlay"
            },
            true
          ]
        }
      },
      {
        "component": "reference",
        "valueType": "string",
        "name": "BackgroundImage",
        "label": "Background Image",
        "description": "Upload Image for the Primary Hero",
        "required": true,
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "imageSlide"
          ]
        }
      },
      {
        "component": "text",
        "valueType": "string",
        "name": "Image Alt",
        "label": "Image Alt Text",
        "description": "Add Alt Text for Image Primary Hero",
        "condition": {
          "===": [
            {
              "var": "PrimarybackgroundType"
            },
            "imageSlide"
          ]
        }
